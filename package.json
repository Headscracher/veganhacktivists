    "@mailchimp/mailchimp_marketing": "^3.0.70",
    "@react-spring/web": "^9.3.0",
    "@tailwindcss/line-clamp": "^0.2.2",
    "classnames": "^2.3.1",
    "contentful": "^9.1.3",
    "crypto-js": "^4.1.1",
    "form-data": "^4.0.0",
    "fuse.js": "^6.4.6",
    "got": "^11.8.2",
    "http-status-codes": "^2.1.4",
    "ky": "^0.28.6",
    "mailgun.js": "^3.6.0",
    "next": "11.1.2",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-hook-form": "^7.17.4",
    "react-select": "^5.1.0",
    "react-toastify": "^8.0.2",
    "react-use-pagination": "^2.0.1",
    "react-waypoint": "^10.1.0",
    "sharp": "^0.29.1",
    "tailwindcss": "^2.2.17"
  },
  "devDependencies": {
    "@axe-core/react": "^4.2.2",
    "@testing-library/dom": "^8.9.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/crypto-js": "^4.0.2",
    "@types/mailchimp__mailchimp_marketing": "^3.0.2",
    "@types/react": "17.0.30",
    "@types/react-gtm-module": "^2.0.1",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.3.0",
    "eslint": "^7.0.0",
    "eslint-config-next": "11.1.2",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.0",
    "lint-staged": "^11.2.3",
    "postcss": "^8.3.9",
    "prettier": "^2.3.2",
    "typescript": "4.4.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jestSetup.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/src/__mocks__/imageMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "optionalDependencies": {
    "contentful-management": "^7.44.1",
    "contentful-typescript-codegen": "^3.2.2"
  }
}
